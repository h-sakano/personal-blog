  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' && stylelint 'src/**/*.{css,jsx,tsx}'",
